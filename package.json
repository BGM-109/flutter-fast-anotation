{
  "name": "sunmkim",
  "displayName": "Flutter fast anotation",
  "description": "Do not leave vscode",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets"
  ],
  "main": "./extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "sunmkim.openDartJ",
        "title": "Open DartJ: Open DartJ"
      },
      {
        "command": "sunmkim.createIndex",
        "title": "ðŸ”¨ Create an index file from which all dart files in this folder are exported"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "sunmkim.createIndex",
          "group": "z_commands"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BGM-109/vs-extension-flutter-anotation-snippet"
  },
  "publisher": "sunmkim",
  "icon": "images/flutbir.png"
}
